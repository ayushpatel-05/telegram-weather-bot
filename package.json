    "start": "npx tsx src/index.ts",
    "dev": "nodemon",
    "build": "npx tsc",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate ./src/migration/%npm_config_migrationName% -d ./src/config/data-source.ts",
