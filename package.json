    "tsx": "^4.19.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@grammyjs/types": "^3.16.0",
    "@types/node": "^22.9.1",
    "@types/pg": "^8.11.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
